@model CST350Milestone5.Models.PlayerRegisterModel

<div>
    <h4>Registration Failed. Username or Email already in use.</h4>
    <hr />
	<dl class="row">
		<dt class="col-sm-2">
			@Html.DisplayNameFor(model => model.Username)
		</dt>
		<dd class="col-sm-10">
			@Html.DisplayFor(model => model.Username)
		</dd>
		<dt class="col-sm-2">
			@Html.DisplayNameFor(model => model.Email)
